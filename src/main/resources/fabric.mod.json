{
	"schemaVersion": 1,
	"id": "burgered",
	"version": "${version}",
	"name": "Burgered",
	"description": "Burgers, Burgers, & More Burgers",
	"authors": [
		"WiredTomato"
	],
	"contact": {
		"homepage": "https://modrinth.com/mod/burgered",
		"sources": "https://github.com/wired-tomato/burgered"
	},
	"license": "MIT",
	"icon": "assets/burgered/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "net.wiredtomato.burgered.Burgered::commonInit",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "net.wiredtomato.burgered.client.BurgeredClient::clientInit",
				"adapter": "kotlin"
			}
		],
		"fabric-datagen": [
			{
				"value": "net.wiredtomato.burgered.data.gen.BurgeredData",
				"adapter": "kotlin"
			}
		],
		"emi": [
			{
				"value": "net.wiredtomato.burgered.client.compat.emi.EMIPlugin",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			{
				"value": "net.wiredtomato.burgered.client.compat.modmenu.ModMenuEntrypoint"
			}
		]
	},
	"mixins": [
		{
			"config": "burgered.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}